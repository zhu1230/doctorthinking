if(!logged_in? )
  page.insert_html 'bottom','post_vote_'+params[:id],:partial => 'shared/ajax_message',:locals => {:message => "您需要  "+(link_to '登录',login_url,:style=>'font-size: 1.0em;color:red')+"  后才能使用投票功能。"}
  page.call 'voteupback','post_vote_'+params[:id]
elsif(current_user.voted_on?(BingliComment.find(params[:id])))
   page.insert_html 'bottom','post_vote_'+params[:id],:partial => 'shared/ajax_message',:locals => {:message => "此前您已经对此病例投过票了。"}
  page.call 'voteupback','post_vote_'+params[:id]
else
  current_user.vote_for(BingliComment.find(params[:id]))
end